{
  "component_types": ["sink", "source"],

  "description": "This is a connector for getting data from TimeBase to Apache Kafka and from Kafka into TimeBase.\nIt is built off of the Kafka Connect framework, and therefore automatically supports pluggable encoding converters, single message transforms, and other useful features.",

  "docker_image": {
    "tag": "@pluginVersion@",
    "name": "timebase-kafka-connector",
    "namespace": "epam",
    "registry":  [ "epam.timebase.connector" ]
  },

  "documentation_url": "https://epam.sharepoint.com/:w:/r/sites/EPAMRTCLab/_layouts/15/doc2.aspx?sourcedoc=%7Ba65fe0c9-89a0-401c-91b9-4a9abc1756b2%7D&action=edit&wdPid=38c8c841&cid=a7b24307-fb33-44ec-8760-dd2e6db527fc",

  "features": {
    "confluent_control_center_integration": true,
    "delivery_guarantee": ["at_least_once"],
    "kafka_connect_api": true,
    "single_message_transforms": true,
    "supported_encodings": ["any"]
  },

  "license": [
    {
      "name": "Apache License, Version 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0",
      "logo": "assets/apache_logo.gif"
    }
  ],

  "logo": "assets/timebase_logo.png",

  "name": "timebase-kafka-connector",

  "owner": {
    "logo": "assets/epam_logo.png",
    "name": "EPAM, Inc.",
    "type": "organization",
    "url": "https://www.epam.com/",
    "username": "epam"
  },

  "requirements": [
    "TimeBase 5.4"
  ],

  "support": {
    "logo": "assets/epam_logo.png",
    "provider_name": "EPAM, Inc.",
    "summary": "We provide full support for this connector.",
    "url": "https://www.epam.com/"
  },

  "tags": [
    "timebase"
  ],

  "title": "TimeBase Kafka Connector",

  "version": "@pluginVersion@"
}
